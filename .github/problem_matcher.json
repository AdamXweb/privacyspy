{
    "problemMatcher": [
        {
            "owner": "automated-tests",
            "pattern": [
                {
                    "regexp": "(Error|Warning|Info)(\\s+)(file=(\\S+)(\\s+))?(line=(\\d+)(\\s+))?(message=(.+))",
                    "file": 4,
                    "severity": 1,
                    "message": 10,
                    "line": 7
                }
            ]
        }
    ]
}